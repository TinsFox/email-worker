{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "mail-box",
	"main": "src/index.ts",
	"dev": {
		"port": 2111
	},
	"workers_dev": false,
	"compatibility_flags": ["nodejs_compat"],
	"compatibility_date": "2025-02-12",
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
		{
			"binding": "R2_BUCKET",
			"bucket_name": "mail"
		}
	],
	"hyperdrive": [
		{
			"binding": "HYPERDRIVE",
			"id": "09a713446a1e4f989983b738952afc83"
		}
	]
}
