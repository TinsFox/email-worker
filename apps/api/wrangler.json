{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "mail-box",
	"main": "src/index.ts",
	"dev": {
		"port": 2111
	},
	"compatibility_flags": [
		"nodejs_compat",
		"nodejs_compat_populate_process_env"
	],
	"compatibility_date": "2025-02-12",
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
		{
			"binding": "R2_BUCKET",
			"bucket_name": "mail"
		}
	]
}
